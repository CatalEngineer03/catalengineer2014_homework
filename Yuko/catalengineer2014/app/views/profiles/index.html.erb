<h1>Listing profiles</h1>

<table>
    <thead>
      <tr>
       <th>Name</th>
       <th>Avatar</th>
      </tr>
    </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
       <td><%= profile.title %></td>
       <td><%= profile.body %></td>
       <td><%=link_to 'Show', profile_path(profile.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New Profile', new_profile_path %>
